{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Image Carousel, Item",
  "name": "elevation/interior-components--image-carousel",
  "version": "1.0.0",
  "category": "elevation-blocks",
  "description": "Image Container",
  "textdomain": "elevation",
  "ancestor": [
    "elevation/interior-components--image-carousel-fixed-with-hover",
    "elevation/interior-components--image-carousel-horizontal-centered",
    "elevation/interior-components--image-carousel-horizontal-left-aligned"
  ],
  "supports": {
    "html": true,
    "align": false,
    "alignWide": false,
    "anchor": true,
    "color": {
      "background": true,
      "gradients": false,
      "text": false,
      "link": false,
      "border": false
    },
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ]
    },
    "interactivity": {
      "clientNavigation": true
    }
  },
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "preview": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "editorScript": [
    "file:index.js"
  ],
  "style": [
    "file:style-index.css"
  ],
  "editorStyle": [
    "file:index.css"
  ]
}
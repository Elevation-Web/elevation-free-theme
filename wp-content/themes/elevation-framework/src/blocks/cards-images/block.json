{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Cards | Layout: Background images + icons",
	"name": "elevation/cards-images",
	"version": "1.0.0",
	"category": "elevation-blocks",
	"description": "Cards with title, description, background images and icons",
	"textdomain": "elevation",
	"parent": ["elevation/cards-wrapper"],
	"supports": {
		"html": true,
		"align": false,
		"alignWide": false,
		"color": {
			"background": true,
			"gradients": false,
			"text": false,
			"link": false,
			"border": false
		},
		"spacing": {
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"preview": {
			"type": "boolean",
			"default": false
		},
		"img": {
			"type": "object",
			"default": {
				"url": "",
				"alt": "",
				"id": 0,
				"srcset": "",
				"width": 0,
				"height": 0,
				"sizes": "",
				"focalPoint": {
					"x": 0.5,
					"y": 0.5
				}
			}
		}
	},
	"editorScript": ["file:index.js"],
	"editorStyle": ["file:index.css"]
}

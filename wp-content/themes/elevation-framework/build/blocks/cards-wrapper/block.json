{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Cards",
  "name": "elevation/cards-wrapper",
  "version": "1.0.0",
  "category": "elevation-blocks",
  "icon": "button",
  "description": "Cards container.",
  "textdomain": "elevation",
  "allowedBlocks": [
    "elevation/cards-images"
  ],
  "supports": {
    "html": true,
    "anchor": true,
    "alignWide": false,
    "ariaLabel": false,
    "color": {
      "background": false,
      "gradients": false,
      "text": false,
      "link": false,
      "border": false
    },
    "spacing": {
      "margin": [
        "top",
        "bottom"
      ],
      "blockGap": false
    },
    "layout": {
      "allowSwitching": false,
      "allowInheriting": false,
      "default": {
        "type": "flex",
        "verticalAlignment": "stretch"
      }
    },
    "interactivity": {
      "clientNavigation": true
    }
  },
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "align": {
      "type": "array",
      "default": "full"
    },
    "is_slider": {
      "type": "boolean",
      "default": true
    },
    "preview": {
      "type": "boolean",
      "default": false
    }
  },
  "example": {
    "attributes": {
      "preview": true
    }
  },
  "editorScript": [
    "file:index.js"
  ],
  "style": [
    "file:style-index.css"
  ],
  "editorStyle": [
    "file:index.css"
  ],
  "viewScript": [
    "file:./view.js"
  ]
}
{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Video Carousel Full Width Item",
	"name": "elevation/interior-components--video-carousel-full-width-item",
	"version": "1.0.0",
	"category": "elevation-blocks",
	"icon": "video-alt3",
	"description": "Video Carousel Full Width Item",
	"textdomain": "elevation",
	"ancestor": ["elevation/interior-components--video-carousel-full-width"],
	"supports": {
		"html": true,
		"anchor": true,
		"align": ["full"],
		"alignWide": false,
		"color": {
			"background": true,
			"gradients": false,
			"text": true
		},
		"spacing": {
			"margin": ["top", "bottom"]
		}
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"preview": {
			"type": "boolean",
			"default": false
		},
		"video": {
			"type": "object",
			"default": {
				"url": "",
				"videoType": "youtube"
			}
		},
		"buttonLabel": {
			"type": "string",
			"default": "Caption. Maximum 10 words. Sem viverra aliquet eget sit amet tellus cras adipiscing enim."
		},
		"imgMobile": {
			"type": "object",
			"default": {
				"url": "",
				"alt": "",
				"height": "500",
				"width": "500",
				"focalPoint": {
					"x": 0.5,
					"y": 0.5
				}
			}
		},
		"imgTablet": {
			"type": "object",
			"default": {
				"url": "",
				"alt": "",
				"height": "500",
				"width": "500",
				"focalPoint": {
					"x": 0.5,
					"y": 0.5
				}
			}
		},
		"imgDesktop": {
			"type": "object",
			"default": {
				"url": "",
				"alt": "",
				"height": "500",
				"width": "500",
				"focalPoint": {
					"x": 0.5,
					"y": 0.5
				}
			}
		}
	},
	"example": {
		"attributes": {
			"preview": true
		}
	},
	"editorScript": ["file:index.js"],
	"style": ["file:style-index.css"],
	"editorStyle": ["file:index.css"]
}

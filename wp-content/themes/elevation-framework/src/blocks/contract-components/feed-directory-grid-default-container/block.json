{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Feed, Layout: Directory Grid Default",
	"name": "elevation/contract-components--feed-directory-grid-default-container",
	"version": "1.0.0",
	"category": "elevation-blocks",
	"description": "Feed, Layout: Directory Grid Default",
	"textdomain": "elevation",
	"parent": ["elevation/interior-components--custom-container"],
	"supports": {
		"anchor": true,
		"align": false,
		"alignWide": false,
		"renaming": false,
		"inserter": true,
		"interactivity": {
			"clientNavigation": true
		}
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"headingLevel": {
			"type": "string",
			"default": "h5"
		},
		"filterBy": {
			"type": "string",
			"default": "latest"
		},
		"postType": {
			"type": "string",
			"default": "resources"
		},
		"categorySelected": {
			"type": "array",
			"default": []
		},
		"taxonomy": {
			"type": "string",
			"default": "resources_categories"
		},
		"filterEnabled": {
			"type": "boolean",
			"default": true
		},
		"postsPerPage": {
			"type": "number",
			"default": 3
		},
		"cardDirectory": {
			"type": "boolean",
			"default": true
		},
		"urlDirectory": {
			"type": "string",
			"default": "/directory"
		},
		"textDirectory": {
			"type": "string",
			"default": "See more from"
		},
		"titleDirectory": {
			"type": "string",
			"default": "Title Page (Directory)"
		},
		"postExclude": {
			"type": "array",
			"default": []
		},
		"showDate": {
			"type": "boolean",
			"default": false
		},
		"withBorder": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": ["file:index.js"],
	"style": ["file:style-index.css"],
	"editorStyle": ["file:index.css"],
	"render": "file:./render.php"
}

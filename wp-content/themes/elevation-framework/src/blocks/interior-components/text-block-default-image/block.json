{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"title": "Text Block, Layout: Default w/ Image.",
	"name": "elevation/interior-components--text-block-default-image",
	"version": "1.0.0",
	"category": "elevation-blocks",
	"description": "Text Block, Layout: Default w/ Image.",
	"textdomain": "elevation",
	"supports": {
		"html": true,
		"align": false,
		"alignWide": false,
		"color": {
			"background": true,
			"gradients": false,
			"text": false,
			"link": false,
			"border": false
		},
		"spacing": {
			"margin": ["top", "bottom"],
			"padding": ["top", "right", "bottom", "left"]
		}
	},
	"attributes": {
		"id": {
			"type": "string",
			"default": ""
		},
		"preview": {
			"type": "boolean",
			"default": false
		},
		"imageSize": {
			"type": "string",
			"default": "small"
		},
		"columnsDirection": {
			"type": "string",
			"default": "row"
		},
		"verticalAlignment": {
			"type": "string",
			"default": "center"
		},
		"cropImage": {
			"type": "boolean",
			"default": false
		}
	},
	"editorScript": ["file:index.js"],
	"style": ["file:style-index.css"],
	"editorStyle": ["file:index.css"]
}

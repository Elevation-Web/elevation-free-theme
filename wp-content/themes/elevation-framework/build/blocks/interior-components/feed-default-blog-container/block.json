{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "title": "Feed Blog Container",
  "name": "elevation/interior-components--feed-default-blog-container",
  "version": "1.0.0",
  "category": "elevation-blocks",
  "description": "Feed, Layout: Default Container",
  "textdomain": "elevation",
  "supports": {
    "anchor": true,
    "align": false,
    "alignWide": false,
    "renaming": false,
    "inserter": true,
    "interactivity": {
      "clientNavigation": true
    }
  },
  "attributes": {
    "id": {
      "type": "string",
      "default": ""
    },
    "headingLevel": {
      "type": "string",
      "default": "h5"
    },
    "filterBy": {
      "type": "string",
      "default": "latest"
    },
    "categorySelected": {
      "type": "array",
      "default": []
    },
    "postsSelected": {
      "type": "array",
      "default": []
    },
    "postsPerPage": {
      "type": "number",
      "default": 6
    }
  },
  "editorScript": [
    "file:index.js"
  ],
  "style": [
    "file:style-index.css"
  ],
  "editorStyle": [
    "file:index.css"
  ],
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}